    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="apple-touch-icon" sizes="152x152" href="/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
    <link rel="manifest" href="/manifest.json">
    <link rel="mask-icon" href="/safari-pinned-tab.svg" color="#5bbad5">
    <meta name="theme-color" content="#ffffff">

    <title>@Model.PageTitle</title>
    
    <style type="text/css">
        @@font-face {
            font-family: 'Gotham Book';
            src: url('../../../frame/fonts/gotham-book-webfont.eot');
            src: url('../../../frame/fonts/gotham-book-webfont.eot?#iefix') format('embedded-opentype'), url('../../../frame/fonts/gotham-book-webfont.woff') format('woff'), url('../../../frame/fonts/gotham-book-webfont.ttf') format('truetype'), url('../../../frame/fonts/gotham-book-webfont.svg') format('svg');
        }

        @@font-face {
            font-family: 'Gotham Medium';
            src: url('../../../frame/fonts/gotham-medium-webfont.eot');
            src: url('../../../frame/fonts/gotham-medium-webfont.eot?#iefix') format('embedded-opentype'), url('../../../frame/fonts/gotham-medium-webfont.woff') format('woff'), url('../../../frame/fonts/gotham-medium-webfont.ttf') format('truetype'), url('../../../frame/fonts/gotham-medium-webfont.svg') format('svg');
        }

        @@font-face {
            font-family: 'Font Awesome';
            src: url('../../../frame/fonts/fontawesome-webfont.eot');
            src: url('../../../frame/fonts/fontawesome-webfont.eot?#iefix') format('embedded-opentype'), url('../../../frame/fonts/fontawesome-webfont.woff') format('woff'), url('../../../frame/fonts/fontawesome-webfont.ttf') format('truetype'), url('../../../frame/fonts/fontawesome-webfont.svg') format('svg');
        }

        @@font-face {
            font-family: 'Glyphicons Halflings';
            src: url('../../../frame/fonts/glyphicons-halflings-regular.eot');
            src: url('../../../frame/fonts/glyphicons-halflings-regular.eot?#iefix') format('embedded-opentype'), url('../../../frame/fonts/glyphicons-halflings-regular.woff') format('woff'), url('../../../frame/fonts/glyphicons-halflings-regular.ttf') format('truetype'), url('../../../frame/fonts/glyphicons-halflings-regular.svg') format('svg');
        }
    </style>

    



    @*@Scripts.Render("~/frame/scripts/modernizr")*@
    <script src="https://mowido.blob.core.windows.net/scripts/modernizr-2.6.2.min.js" type="text/javascript"></script>

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

    @Styles.Render("~/frame/css")
    @*<link href="https://mowido.blob.core.windows.net/css/site.1.2.1.min.css" rel="stylesheet" />*@

    <link rel="shortcut icon" href="~/favicon.ico">
    @RenderSection("Header", required: false)
    
</head>
<body itemscope itemtype="http://schema.org/WebPage">
     <!-- Google Code for Remarketing Tag -->
    <!--------------------------------------------------
    Remarketing tags may not be associated with personally identifiable information or placed on pages related to sensitive categories. See more information and instructions on how to setup the tag on: http://google.com/ads/remarketingsetup
    --------------------------------------------------->
    <script type="text/javascript">
                /* <![CDATA[ */
                var google_conversion_id = 828305870;
                var google_custom_params = window.google_tag_params;
                var google_remarketing_only = true;
                /* ]]> */
    </script>
    <script type="text/javascript" src="//www.googleadservices.com/pagead/conversion.js">
    </script>
    <noscript>
        <div style="display:inline;">
            <img height="1" width="1" style="border-style:none;width:1px;height:1px;" alt="" src="//googleads.g.doubleclick.net/pagead/viewthroughconversion/828305870/?guid=ON&amp;script=0" />
        </div>
    </noscript>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <div class="logo-area">
                    <a href="/">
                        <img src="https://mowido.blob.core.windows.net/icons/logo-white-new.png" alt="Mowido" />
                    </a>
                </div>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li><a href="/@Resources.Resources.Url_FindHouse.ToLower()">@Resources.Resources.TopMenu_FindHouse</a></li>
                    <li><a href="@Resources.Resources.Url_BuyingHome">@Resources.Resources.TopMenu_BuyingHome</a></li>
                    <li><a href="@Resources.Resources.Url_About">@Resources.Resources.About_Header</a></li>
                    
                </ul>
            </div>
        </div>
    </div>

    @RenderSection("TopBody", required: false)

    <div class="container body-content test">
        @RenderBody()
    </div>

        <footer>
            <div class="footer-border">

            </div>
            <div class="footer-area">
                <div class="container">
                    <div class="row">
                        <div class="col-md-4">
                            <div class="logo-footer-area">
                                <a href="/">
                                    <img src="https://mowido.blob.core.windows.net/icons/temp-logo-blue.png" alt="Mowido" />
                                </a>
                                <p>Copyright © 2017 Mowido</p>
                            </div>
                        </div>
                        <div class="col-md-1"></div>
                        <div class="col-md-3 footer-col">
                            <h2>@Resources.Resources.Top5Country</h2>
                            <ul>
                                <li><a href="@Common.Helpers.UrlBuilder.PlaceUrl(6, "france", Resources.Resources.Url_FindHouse)">@Resources.Resources.France</a></li>
                                <li><a href="@Common.Helpers.UrlBuilder.PlaceUrl(9, "italy", Resources.Resources.Url_FindHouse)">@Resources.Resources.Italy</a></li>
                                <li><a href="@Common.Helpers.UrlBuilder.PlaceUrl(16, "portugal", Resources.Resources.Url_FindHouse)">@Resources.Resources.Portugal</a></li>
                                <li><a href="@Common.Helpers.UrlBuilder.PlaceUrl(1, "spain", Resources.Resources.Url_FindHouse)">@Resources.Resources.Spain</a></li>
                                <li><a href="@Common.Helpers.UrlBuilder.PlaceUrl(25, "thailand", Resources.Resources.Url_FindHouse)">@Resources.Resources.Thailand</a></li>
                            </ul>
                        </div>
                        <div class="col-md-2 footer-col">
                            <h2>@Resources.Resources.Top5City</h2>
                            <ul>
                                <li><a href="@Common.Helpers.UrlBuilder.PlaceUrl(27, "cannes", Resources.Resources.Url_FindHouse)">@Resources.Resources.Cannes</a></li>
                                <li><a href="@Common.Helpers.UrlBuilder.PlaceUrl(26, "koh-lanta", Resources.Resources.Url_FindHouse)">@Resources.Resources.KohLanta</a></li>
                                <li><a href="@Common.Helpers.UrlBuilder.PlaceUrl(20, "lisboa", Resources.Resources.Url_FindHouse)">@Resources.Resources.Lisboa</a></li>
                                <li><a href="@Common.Helpers.UrlBuilder.PlaceUrl(4, "malaga", Resources.Resources.Url_FindHouse)">@Resources.Resources.Malaga</a></li>
                                <li><a href="@Common.Helpers.UrlBuilder.PlaceUrl(30, "nice", Resources.Resources.Url_FindHouse)">@Resources.Resources.Nice</a></li>
                            </ul>
                        </div>
                        <div class="col-md-2 social-media">
                            <a href="https://www.facebook.com/mowido.se">
                                <img class="fb-img" src="https://mowido.blob.core.windows.net/icons/fb-icon-blue.png" alt="Facebook Mowido"  />
                            </a>
                            <a href="https://www.instagram.com/mowido.se/">
                                <img class="insta-img" src="https://mowido.blob.core.windows.net/icons/insta-icon-blue.png" alt="Instagram Mowido" />
                            </a>
                        </div>
                    </div>
                </div>
            </div>

            
        </footer>


        @*@Scripts.Render("~/frame/scripts")
        @Scripts.Render("~/frame/scripts/bootstrap")*@
        <script src="https://mowido.blob.core.windows.net/scripts/scripts.1.0.2.min.js" type="text/javascript"></script>
        @RenderSection("scripts", required: false)
</body>
</html>
